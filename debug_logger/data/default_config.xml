<odoo>
    <data noupdate="1">
        <!-- Default logging configuration -->
        <record id="default_debug_logger_settings" model="debug.logger.settings">
            <field name="name">Default Configuration</field>
            <field name="active">true</field>
            <field name="enable_logging">true</field>
            <field name="store_entries">false</field>
            <field name="max_entries">1000</field>
            <field name="log_create">true</field>
            <field name="log_write">true</field>
            <field name="log_unlink">true</field>
            <field name="log_search">false</field>
            <field name="log_search_read">false</field>
        </record>
    </data>
</odoo>