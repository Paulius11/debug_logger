<odoo>
    <data noupdate="1">
        <!-- Default parameters for Debug Logger -->
        <record id="param_debug_logger_enable" model="ir.config_parameter">
            <field name="key">debug_logger.enable</field>
            <field name="value">False</field>
        </record>
        <record id="param_debug_logger_methods" model="ir.config_parameter">
            <field name="key">debug_logger.methods</field>
            <field name="value">create,write</field>
        </record>
        <record id="param_debug_logger_models" model="ir.config_parameter">
            <field name="key">debug_logger.models</field>
            <field name="value">all</field>
        </record>
    </data>
</odoo>
